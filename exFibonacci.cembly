START

    INT n = 0;
    INT first = 0;
    INT second = 1;
    INT result = 0;
    INT i = 0;

    WRITE("Print the first N digits of the Fibonacci sequence", INT n);

    READ_INT(n);

    LOOP (i < n) BEGIN
        n = n - 1;
        result = i;
        IF (i <= 1) BEGIN
            result = i;
        END
        ELSE BEGIN
            result = first + second;
            first = second;
            second = result;
        END;
    END;

    WRITE("Resultado ", INT result);

FINISH