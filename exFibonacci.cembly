START

    INT n = 0;
    INT first = 0;
    INT second = 1;
    INT result = 0;
    INT i = 0;

    WRITE("Print the first N digits of the Fibonacci sequence. N?");

    READ_INT(n);

    LOOP (i < n) BEGIN
        IF (i <= 1) BEGIN
            result = i;
            i = i + 1;
        END;
        ELSE BEGIN
            result = first + second;
            first = second;
            second = result;
            i = i + 1;
        END;
    END;

    WRITE("Resultado ", INT result);

FINISH
